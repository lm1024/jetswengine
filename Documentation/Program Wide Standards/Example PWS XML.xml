<?xml version="1.0"?>

<slideshow>
	<documentinfo>
		<author>Tom Davidson</author>
		<version>1.0</version>
		<comment>this is a comment</comment>
		<groupid>2</groupid>
	</documentinfo>
	<defaultsettings>
		<backgroundcolor>#ff00ff00</backgroundcolor>
		<font>times new roman</font>
		<fontsize>24</fontsize>
		<fontcolor>#ffcccc00</fontcolor>
		<graphiccolor>#ffaaaa00</graphiccolor>
	</defaultsettings>
	<slide>
		<text sourcefile="null" xstart="0.5" ystart="0.5" font="arial" fontsize="24" fontcolor="#00112233"  duration="1.5">
			Some Text.
		</text>
		
		<text xstart="0.5" ystart="0.5" font="arial" fontsize="24" fontcolor="#00112233" duration="1.5" starttime="2.0">
			<richtext font="arial" fontsize="26" fontcolor="#11223344" newline="true" b="true" i="true" u="true" strikethrough="true" superscript="true" subscript="true" case="upper/lower/camel" alignment="left/right/centre">Example text!</richtext>
			<richtext font="arial" fontsize="26" fontcolor="#11223344" newline="true" b="true" i="true" u="true" strikethrough="true" superscript="true" subscript="true" case="upper/lower/camel" alignment="left">Example text!</richtext>
			<richtext font="arial" fontsize="26" fontcolor="#11223344" newline="true" b="true" i="true" u="true" strikethrough="true" superscript="true" subscript="true" case="upper/lower/camel" alignment="left">Example text!</richtext>
		</text>
		
		<text xstart="0.5" ystart="0.5" font="arial" fontsize="24" fontcolor="#00112233" duration="1.5" starttime="2.0">
			<richtext font="arial" fontsize="26" fontcolor="#11223344" newline="true" b="true" i="true" u="true" strikethrough="true" superscript="true" subscript="true" case="upper/lower/camel" alignment="left">Example text!</richtext>
			<richtext font="arial" fontsize="26" fontcolor="#11223344" newline="true" b="true" i="true" u="true" strikethrough="true" superscript="true" subscript="true" case="upper/lower/camel" alignment="left">Example text!</richtext>
		</text>

		<image sourcefile="cat.jpg" xstart="0.5" ystart="0.5" scale="1.0" duration="1.5" starttime="2.0" />
		
		<image sourcefile="selfie.jpg" xstart="0.5" ystart="0.5" scale="1.0" duration="1.5" starttime="2.0" rotate="90" fliphorizontal="true" flipvertical="true" cropx1="0.0" cropy1="0.0" cropx2="0.5" cropy2="0.5"/>
		
		<audio sourcefile="gavel.wav" starttime="2.5" />
		<video sourcefile="avengers.mkv" xstart="0.5" ystart="0.5" />
		
		<!-- PWS Shapes -->
		<graphic type="oval/rectangle/line" xstart="0.5" ystart="0.5" xend="0.7" yend="0.7" solid="true" graphiccolor="#11223344" duration="1.5">
			<cyclicshading shadingcolor="#88776655" />
		</graphic>		
		

		<!-- Our Expanded PWS Shapes -->
		<graphic graphiccolor="#11223344" duration="1.5" starttime="2.0">
			<oval xstart="0.5" ystart="0.5" xend="0.7" yend="0.7" solid="true" outlinecolor="#00000000" outlinethickness="1.0" rotation="45"/>
			<shadow weight="heavy/normal/light"/>
			<cyclicshading shadingcolor="#88776655">
				<shading shadingcolor="#00aabbcc"/>
				<shading shadingcolor="#00aabbcc"/>
			<cyclicshading/>
		</graphic>
		
		<graphic graphiccolor="#11223344" duration="1.5" starttime="2.0">
			<rectangle xstart="0.5" ystart="0.5" xend="0.7" yend="0.7" arcwidth="10" archeight="10" solid="true" outlinecolor="#00000000" outlinethickness="1.0" rotation="45"/>
		</graphic>
		
		<graphic graphiccolor="#11223344" duration="1.5" starttime="2.0">
			<line xstart="0.5" ystart="0.5" xend="0.7" yend="0.7" outlinethickness="1.0"/> <!-- I'm happy with calling this outline thickness... -->
		</graphic>
		
		
		<!-- Our Added Shapes -->
		
		<graphic graphiccolor="#11223344" duration="1.5" starttime="2.0">
			<circle xstart="0.5" ystart="0.5" size="10" solid="true" outlinecolor="#00000000" outlinethickness="1.0"/>
			<shadow weight="heavy/normal/light"/>
			<cyclicshading shadingcolor="#88776655">
				<shading shadingcolor="#00aabbcc"/>
				<shading shadingcolor="#00aabbcc"/>
			<cyclicshading/>
		</graphic>
		
		<graphic graphiccolor="#11223344" duration="1.5" starttime="2.0">
			<square xstart="0.5" ystart="0.5" size="10" solid="true" outlinecolor="#00000000" outlinethickness="1.0">
		</graphic>
		
		<graphic graphiccolor="#11223344" duration="1.5" starttime="2.0">
			<itriangle xstart="0.5" ystart="0.5" xend="0.7" yend="0.7" solid="true"/>
		</graphic>
	</slide>
</slideshow>
